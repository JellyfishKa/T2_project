# ==========================================
# T2 Logistics - Production Environment
# ==========================================
# Copy this file to .env and fill in your values

# ==========================================
# Database Configuration
# ==========================================
DATABASE_USER=postgres
DATABASE_PASSWORD=your_secure_password_here
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=t2

# ==========================================
# LLM Models Configuration
# ==========================================
# Qwen (Primary Model)
QWEN_API_ENDPOINT=local
QWEN_MODEL_ID=qwen2-0_5b-instruct-q4_k_m.gguf

# Llama (Fallback Model)
LLAMA_API_ENDPOINT=local
LLAMA_MODEL_ID=Llama-3.2-1B-Instruct-Q4_K_M.gguf

# ==========================================
# Hugging Face (optional)
# ==========================================
HF_TOKEN=your_hf_token_here

# ==========================================
# Application Settings
# ==========================================
DEBUG=false
ENVIRONMENT=production

# ==========================================
# CORS (comma-separated origins, empty = defaults)
# ==========================================
CORS_ORIGINS=http://localhost,http://localhost:5173,http://100.120.184.98

# ==========================================
# Server Settings
# ==========================================
BACKEND_PORT=8000
FRONTEND_PORT=5173